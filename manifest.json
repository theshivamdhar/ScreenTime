{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.1",
  "description": "Tracks time spent on websites.",
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "dashboard.html"
}
